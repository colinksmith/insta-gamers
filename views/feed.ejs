<%- include('partials/header') -%>
  <main class="container-fluid">
    <div class="row justify-content-center">
      <%- include('partials/sidebar') -%>
      <div class="col-md-10 offset-md-2 justify-content-center">
        <ul class="row gx-0 gx-md-1 list-unstyled">
          <% for(var i=0; i<posts.length; i++) {%>
            <li class="col-6 justify-content-between align-items-center pb-md-2">
              <a href="/post/<%= posts[i]._id%>">
                <img class="img-fluid feed-image hover-effect" src="<%= posts[i].image%>">
              </a>
            </li>
          <% } %>
          <% if (posts.length === 0) { %>
            <div class="vh-100 d-flex flex-column justify-content-center align-items-center">
              <p class="text-center text-">Looks like you're not following anyone</p>

            </div>
          <% } %>
          </ul>
      </div>
    </div>
  </main>  
<%- include('partials/footer') -%>